  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr -o",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "platform": "npx cap add android && npx cap add ios && npx cap add @capacitor-community/electron && cordova-res android --type splash --skip-config --copy && cordova-res ios --skip-config --copy",
    "sync": "npx jetifier && npx cap sync android && npx cap sync @capacitor-community/electron && npx cap sync ios ",
    "ios": "npx cap open ios",
    "android": "npx cap open android",
    "mac": "cd electron && npm i && npm run electron:build-mac && cd ..",
    "win32": "cd electron && npm i && npm run electron:build-windows-32 && cd ..",
    "win64": "cd electron && npm i && npm run electron:build-windows-64 && cd .."
